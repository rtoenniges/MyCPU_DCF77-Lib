                ;--------------------------------------------------------- 
                ;Interrupt routines   
                ;---------------------------------------------------------       
                
                ;Receiver interrupt        
                int_dcf77
    8136 3001               LDA #1 
    8138 420880             STAA FLG_dcfReceiver ;Flank detected -> Set flag
    813B 7D0A80             INCA VAR_edgeCnt ;Count edges (For signal error detection)
    813E 1F                 RTS       
                
                ;Timer interrupt
                int_timer
                            ;Measure time between two edges
    813F 320880             LDA FLG_dcfReceiver
    8142 184881             JNZ decode       
    8145 7C10               INC ZP_temp1
    8147 1F                 RTS
                
                ;--------------------------------------------------------- 
                ;DCF77 decoding   
                ;---------------------------------------------------------
                decode 
                ;From this point no interrupt should break the programm
    8148 05                 SEC
    8149 1BA802             JSR (KERN_SPINLOCK) ;"You shall not pass"           
                
                ;Synchronize with signal -> Detect syncpoint/-gap
    814C 3110               LDA ZP_temp1
    814E 7028               CMP #PARAM_SYNCPAUSE  
    8150 165F81             JNC _dec0
                ;Time >= PARAM_SYNCPAUSE -> Time longer than 1 second
                ;Syncpoint reached
    8153 2F0980             STZ FLG_synced
    8156 2F0D80             STZ VAR_second
    8159 2F0A80             STZ VAR_edgeCnt
    815C 109E83             JMP _decEnd
                
                ;Time < PARAM_SYNCPAUSE -> New second or bit information     
                ;Count seconds, Check signal for errors   
    815F 7014   _dec0       CMP #PARAM_SECOND 
    8161 166A81             JNC newBit
                            ;Time >= PARAM_SECOND -> Next second
    8164 7D0D80             INCA VAR_second  
    8167 109E83             JMP _decEnd
                
                ;Time < PARAM_SECOND -> New bit 
                newBit 
                ;Display synced status on I/O-Module LEDs
    816A 1AA983             JSR syncDisp
                ;Display synced status on SCC-Board
                ;First do signal checking -> Twice as many edges+1 as seconds?
    816D 320A80             LDAA VAR_edgeCnt
    8170 05                 SEC
    8171 9001               SBC #1
    8173 B002               DIV #2
    8175 720D80             CMPA VAR_second
    8178 198B81             JPZ _nBit0 ;Check successfull -> Go forward to bit checking
                
                ;No longer synchronized        
                deSync  
    817B 3001               LDA #1 
    817D 420980             STAA FLG_synced
    8180 2F0B80             STZ VAR_dataOK
    8183 3008               LDA #08
    8185 421B80             STAA VAR_ledsDataOK
    8188 109E83             JMP _decEnd
                
                ;Decode bit     
    818B 320980 _nBit0      LDAA FLG_synced
    818E 189E83             JNZ _decEnd
                            ;Only continue if synchronized
                
    8191 320D80             LDAA VAR_second
    8194 18DC81             JNZ _nBit3
    8197 1A1C84             JSR getBit
    819A 187B81             JNZ deSync ;If Bit 0 != 0 -> Not synchronized or incorrect signal
                
                ;Second/bit = 0 -> Take over data from last minute            
    819D 320B80             LDAA VAR_dataOK
    81A0 D001               AND #01h
    81A2 19AB81             JPZ _nBit1
    81A5 321580             LDAA VAR_tmpMinutes ;Take over 'minutes'
    81A8 420E80             STAA VAR_minutes
    81AB 320B80 _nBit1      LDAA VAR_dataOK
    81AE D002               AND #02h
    81B0 19B981             JPZ _nBit2
    81B3 321680             LDAA VAR_tmpHours ;Take over 'hours'
    81B6 420F80             STAA VAR_hours
    81B9 320B80 _nBit2      LDAA VAR_dataOK
    81BC D004               AND #04h
    81BE 199E83             JPZ _decEnd
    81C1 321880             LDAA VAR_tmpWeekday ;Take over 'weekday'
    81C4 421180             STAA VAR_weekday
    81C7 321780             LDAA VAR_tmpDay ;Take over 'day'
    81CA 421080             STAA VAR_day
    81CD 321980             LDAA VAR_tmpMonth ;Take over 'month'
    81D0 421280             STAA VAR_month
    81D3 321A80             LDAA VAR_tmpYear ;Take over 'year'
    81D6 421380             STAA VAR_year
    81D9 109E83             JMP _decEnd
                
    81DC 7014   _nBit3      CMP #20
    81DE 169E83             JNC _decEnd ;Below bit 20 is nothing important
    81E1 18ED81             JNZ _nBit4
    81E4 1A1C84             JSR getBit; Second/bit = 20 -> Begin of time information always '1'
    81E7 197B81             JPZ deSync ;If Bit 20 != 1 -> Not synchronized or incorrect signal
    81EA 109E83             JMP _decEnd
                
                ;Bit >20 - Get/decode data
    81ED 320D80 _nBit4      LDAA VAR_second
    81F0 701D               CMP #29
    81F2 161A82             JNC getMinutes ;Go to minute decoding
                            ;Second >= 29
    81F5 7024               CMP #36
    81F7 166782             JNC getHours ;Go to hour decoding
                            ;Second >= 36
    81FA 702A               CMP #42
    81FC 16B682             JNC getDay ;Go to day decoding
                            ;Second >= 42
    81FF 702D               CMP #45
    8201 16E682             JNC getWDay ;Go to weekday decoding
                            ;Second >= 45
    8204 7032               CMP #50
    8206 161A83             JNC getMonth ;Go to month decoding
                            ;Second >= 50
    8209 703B               CMP #59
    820B 164E83             JNC getYear ;Go to year decoding
    820E 189E83             JNZ _decEnd
                            ;Second = 59 -> Leap second!
    8211 1A1C84             JSR getBit ;Always '0'
    8214 187B81             JNZ deSync 
    8217 109E83             JMP _decEnd
                
                ;Get/decode minutes
                ;---------------------------------------------------------
                getMinutes   
    821A 701C               CMP #28
    821C 193282             JPZ parityMinutes ;Last bit -> Check parity
    821F 7015               CMP #21
    8221 182782             JNZ _gMin0
    8224 481100             MOV ZP_temp1+1,#0 ;First bit -> Clear data
                
                ;Get bit (minutes)
    8227 1A1C84 _gMin0      JSR getBit
    822A E111               ORA ZP_temp1+1
    822C DB                 SHR
    822D 4111               STA ZP_temp1+1
    822F 109E83             JMP _decEnd
                
                ;Last bit
                ;Check parity (minutes)        
                parityMinutes  
    8232 1A1C84             JSR getBit ;Get "Carry-Bit" and save it to stack for later use
    8235 08                 PHA
                            ;Determine if bitcount of data is even or odd
    8236 3111               LDA ZP_temp1+1
    8238 5007               LDX #7
    823A 2E                 CLY
    823B 1A2884             JSR bitCnt
    823E 175082             JPC _pMin0   
    8241 0C                 PLA ;Bit count = "odd"
    8242 185482             JNZ _pMinOK
                
    8245 3006   _pMinBAD    LDA #06h ;Parity n.OK
    8247 D20B80             ANDA VAR_dataOK
    824A 420B80             STAA VAR_dataOK
    824D 109E83             JMP _decEnd
                
    8250 0C     _pMin0      PLA ;Bit count = "even"
    8251 184582             JNZ _pMinBAD
                
    8254 3111   _pMinOK     LDA ZP_temp1+1 ;Parity OK
    8256 1A3D84             JSR bcdToDec
    8259 421580             STAA VAR_tmpMinutes
    825C 3001               LDA #01h
    825E E20B80             ORAA VAR_dataOK
    8261 420B80             STAA VAR_dataOK
    8264 109E83             JMP _decEnd
                
                ;Get/decode hours
                ;---------------------------------------------------------
                getHours
    8267 7023               CMP #35
    8269 197F82             JPZ parityHours ;Last bit -> Check parity
    826C 701D               CMP #29
    826E 187482             JNZ _gHrs0
    8271 481100             MOV ZP_temp1+1,#0 ;First Bit -> Clear data
                
                ;Get bit (hours)
    8274 1A1C84 _gHrs0      JSR getBit
    8277 E111               ORA ZP_temp1+1
    8279 DB                 SHR
    827A 4111               STA ZP_temp1+1 
    827C 109E83             JMP _decEnd
                
                ;Last bit
                ;Check parity (hours)         
                parityHours       
    827F BC11               SHR ZP_temp1+1 ;Shift data right by 1
                
    8281 1A1C84             JSR getBit ;Get "Carry-Bit" and save it to stack for later use
    8284 08                 PHA
                            ;Determine if bitcount of data is even or odd
    8285 3111               LDA ZP_temp1+1  
    8287 5006               LDX #6
    8289 2E                 CLY
    828A 1A2884             JSR bitCnt
    828D 179F82             JPC _pHrs0   
    8290 0C                 PLA ;Bit count = "odd"
    8291 18A382             JNZ _pHrsOK
                
    8294 3005   _pHrsBAD    LDA #05h ;Parity n.OK
    8296 D20B80             ANDA VAR_dataOK
    8299 420B80             STAA VAR_dataOK
    829C 109E83             JMP _decEnd
                
    829F 0C     _pHrs0      PLA ;Bit count = "even"
    82A0 189482             JNZ _pHrsBAD
                
    82A3 3111   _pHrsOK     LDA ZP_temp1+1 ;Parity OK
    82A5 1A3D84             JSR bcdToDec
    82A8 421680             STAA VAR_tmpHours
    82AB 3002               LDA #02h
    82AD E20B80             ORAA VAR_dataOK
    82B0 420B80             STAA VAR_dataOK
    82B3 109E83             JMP _decEnd
                
                ;Get/decode day
                ;---------------------------------------------------------
                getDay  
    82B6 7024               CMP #36 
    82B8 18BE82             JNZ _gDay0
    82BB 481100             MOV ZP_temp1+1,#0 ;First Bit -> Clear data
                
                ;Get bit (day)      
    82BE 1A1C84 _gDay0      JSR getBit
    82C1 E111               ORA ZP_temp1+1
    82C3 DB                 SHR
    82C4 4111               STA ZP_temp1+1  
                            ;Check for last bit
    82C6 320D80             LDAA VAR_second
    82C9 7029               CMP #41       
    82CB 189E83             JNZ _decEnd 
                
                ;Last bit
    82CE BC11               SHR ZP_temp1+1 ;Shift data right by 1 
                
                            ;Count high bits and add it to "VAR_dateParity"
    82D0 3111               LDA ZP_temp1+1  
    82D2 5006               LDX #6
    82D4 2E                 CLY
    82D5 1A2884             JSR bitCnt
    82D8 421480             STAA VAR_dateParity
                            ;Save day value
    82DB 3111               LDA ZP_temp1+1
    82DD 1A3D84             JSR bcdToDec
    82E0 421780             STAA VAR_tmpDay
    82E3 109E83             JMP _decEnd        
                
                ;Get/decode weekday
                ;---------------------------------------------------------
                getWDay 
    82E6 702A               CMP #42
    82E8 18EE82             JNZ _getWDay0
    82EB 481100             MOV ZP_temp1+1,#0 ;First Bit -> Clear data
                
                ;Get bit (weekday)    
    82EE 1A1C84 _getWDay0   JSR getBit
    82F1 E111               ORA ZP_temp1+1
    82F3 DB                 SHR
    82F4 4111               STA ZP_temp1+1 
                            ;Check for last bit
    82F6 320D80             LDAA VAR_second
    82F9 702C               CMP #44       
    82FB 189E83             JNZ _decEnd
                
                ;Last bit
                            ;Shift data right by 4
    82FE 3111               LDA ZP_temp1+1 
    8300 B010               DIV #10h
    8302 4111               STA ZP_temp1+1 
                
                            ;Count high bits and add it to "VAR_dateParity"
    8304 5003               LDX #3
    8306 591480             LDYA VAR_dateParity
    8309 1A2884             JSR bitCnt
    830C 421480             STAA VAR_dateParity
                            ;Save weekday value
    830F 3111               LDA ZP_temp1+1 
    8311 1A3D84             JSR bcdToDec
    8314 421880             STAA VAR_tmpWeekday
    8317 109E83             JMP _decEnd  
                
                ;Get/decode month
                ;---------------------------------------------------------
                getMonth    
    831A 702D               CMP #45
    831C 182283             JNZ _gMon0
    831F 481100             MOV ZP_temp1+1 ,#0 ;First Bit -> Clear data
                
                ;Get bit (month)
    8322 1A1C84 _gMon0      JSR getBit
    8325 E111               ORA ZP_temp1+1 
    8327 DB                 SHR
    8328 4111               STA ZP_temp1+1  
                            ;Check for last bit
    832A 320D80             LDAA VAR_second
    832D 7031               CMP #49       
    832F 189E83             JNZ _decEnd 
                
